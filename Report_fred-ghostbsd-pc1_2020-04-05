/usr/local/etc/X11/xorg.conf.d/vmware.conf






# - - - - - - - - - - - - -

Section "ServerFlags"
Option "AutoAddDevices" "false"
EndSection
Section "InputDevice"
Identifier "Mouse0"
Driver "vmmouse"
Option "Device" "/dev/sysmouse"
EndSection



